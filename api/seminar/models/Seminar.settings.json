{
  "connection": "default",
  "collectionName": "seminar",
  "info": {
    "name": "seminar",
    "description": "Seminar Posts"
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "title": {
      "required": true,
      "type": "string"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "archived": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "division": {
      "model": "division",
      "via": "seminars"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "additionalLinks": {
      "type": "json"
    },
    "slideUrl": {
      "type": "string",
      "default": ""
    },
    "sourceLink": {
      "type": "string",
      "default": ""
    }
  }
}