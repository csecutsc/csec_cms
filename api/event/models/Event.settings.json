{
  "connection": "default",
  "collectionName": "event",
  "info": {
    "name": "event",
    "description": "CSEC Events"
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "startTime": {
      "type": "date",
      "required": true
    },
    "endTime": {
      "type": "date"
    },
    "location": {
      "type": "string",
      "required": true
    },
    "active": {
      "default": true,
      "type": "boolean",
      "required": true
    },
    "cardImage": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "user": {
      "model": "user",
      "via": "events",
      "plugin": "users-permissions"
    }
  }
}